<div class="row">
  <div class="col-lg-12">
    <h1>All Users:</h1>
  </div>
</div>
<div class="row">
  <div class="col-lg-12">
    <h3>Users:</h1>
    <ul>
      <% @users.each do |user| %>
        <li><%= link_to "#{user.name}", user_path(user)%></li>
      <% end %>
    </ul>
  </div>
</div>

<div class="row">
  <div class="col-lg-12 inline">
    <%= link_to "New", new_user_path, class: 'btn btn-success' %>
  </div>
</div>

